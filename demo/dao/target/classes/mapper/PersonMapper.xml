<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.demo.dao.mapper.PersonDao">

    <insert id="insert" parameterType="com.demo.dao.entity.Person">
    INSERT INTO person VALUES (#{id},#{name},#{job},#{gender},#{age},#{salary})
    </insert>

    <select id="findAll" resultType="com.demo.dao.entity.Person">
    SELECT * FROM person
    </select>

</mapper>
